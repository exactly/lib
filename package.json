{
  "name": "@exactly/lib",
  "version": "0.0.1",
  "homepage": "https://exact.ly",
  "repository": "github:exactly/lib",
  "scripts": {
    "prepare": "pnpm build",
    "build": "pkgroll --sourcemap",
    "build:esm": "tsc --module ES2020 --outDir esm && echo '{ \"type\": \"module\" }' > esm/package.json",
    "test": "pnpm run --aggregate-output /test:/",
    "test:vi": "vitest run",
    "test:lint": "eslint ."
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/eslint-plugin": "^1.1.4",
    "@vitest/ui": "^2.1.1",
    "eslint": "maintenance",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^13.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "fast-check": "^3.22.0",
    "pkgroll": "^2.5.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "viem": "^2.21.9",
    "vitest": "^2.1.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "eslint-config-universe": "patches/eslint-config-universe.patch"
    }
  }
}
